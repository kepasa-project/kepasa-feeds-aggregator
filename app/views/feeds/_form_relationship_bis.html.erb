<%= form_for (@relationship) do |f| %>
  
  <div class="field">
  <%= f.fields_for :feed do |rssurl| %>
    <%= rssurl.label :inserta_direccion_rss %><br />
    <%= rssurl.text_field :feed %>
    <% end %>
  </div>
<div class="field">
    <%= f.hidden_field :user_id, :value => current_user.id %>
  </div>
  
<div class="form-actions">
    <%= f.submit 'Insertar Nueva Noticias', class: "btn btn-info btn-rounded" %>
  </div>
<% end %>
