# Ignore bundler config
/.bundle

# Ignore the setting command server file  
dump.rdb

#Vagrant
Vagrantfile
/.vagrant

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp/*
!/tmp/.keep

#Ignore rails 5 new environment
/node_modules
/yarn-error.log
.byebug_history

# assets pipeline
/public/assets/*

#personal pages
/app/views/pages/*

# Ignore application configuration
/config/application.yml
/config/database.yml
/config/secrets.yml
/config/newrelic.yml
/config/initializers/sidekiq.rb
